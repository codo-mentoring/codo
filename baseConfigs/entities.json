    "view:report": {
      "component": "Reporting",
      "config": {
        "reports": [
          {
            "title": "Gesamt-Übersicht",
            "aggregationDefinitions": [
              {
                "label": "Patenschaften neu verknüpft",
                "query": "ChildSchoolRelation:toArray[*start >= ? & start <= ?]"
              },
              {
                "label": "Mentees aufgenommen",
                "query": "Child:toArray[*admissionDate >= ? & admissionDate <= ?]"
              },
              {
                "label": "Mentor:innen aufgenommen",
                "query": "School:toArray[*admissionDate >= ? & admissionDate <= ?]"
              },
              {
                "label": "Aktueller Stand",
                "aggregations": [
                  {
                    "label": "Patenschaften gesamt",
                    "query": "ChildSchoolRelation:toArray",
                    "groupBy": [
                      "status",
                      "foerderungDurch"
                    ]
                  },
                  {
                    "label": "Mentees gesamt",
                    "query": "Child:toArray",
                    "groupBy": [
                      "projectStatus",
                      "gender"
                    ]
                  },
                  {
                    "label": "Mentor:innen gesamt",
                    "query": "School:toArray",
                    "groupBy": [
                      "projectStatus",
                      "gender"
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    
    
    "appConfig:matching-entities": {
      "matchActionLabel": "Pre-Match erstellen",
      "columns": [
        ["name", "name"],
        ["mentoringGeschlecht", "gender"],
        ["mentoringHilfe","mentoringHilfe"],
        ["distance", null]
      ],
      "rightSide": {
          "entityType": "School",
          "availableFilters": [
            { "id": "gender" },
            { "id": "mentoringHilfe" }
          ],
          "prefilter": {
            "$or": [
              { "projectStatus.id": "VORBEREITUNG" },
              { "projectStatus.id": "MATCHING_BEREIT" }
            ]
          }
        },
        "leftSide": {
          "entityType": "Child",
          "availableFilters": [
            { "id": "gender" },
            { "id": "mentoringHilfe" }
          ],
          "prefilter": {
            "$or": [
              { "projectStatus.id": "VORBEREITUNG" },
              { "projectStatus.id": "MATCHING_BEREIT" }
            ]
          }
        },
      "onMatch": {
        "newEntityType": "ChildSchoolRelation",
        "newEntityMatchPropertyLeft": "childId",
        "newEntityMatchPropertyRight": "schoolId",
        "columnsToReview": ["name", "start", "childId", "schoolId"]
      }
    },
    
    "view:matching": {
      "component": "MatchingEntities"
    },
